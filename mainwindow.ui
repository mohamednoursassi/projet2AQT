       <string notr="true">QComboBox {
    /* Style de base pour la ComboBox */
    background-color: #3498db;  /* Couleur de fond bleu */
    color: #ffffff;  /* Couleur du texte blanc */
    border-radius: 10px;  /* Bords arrondis */
    padding: 5px 10px;  /* Marges intérieures */
    font-family: 'Arial', sans-serif;  /* Police Arial */
    font-size: 16px;  /* Taille de la police */
    border: 2px solid #2980b9;  /* Bordure */
    min-width: 150px;  /* Largeur minimale */
}

QComboBox:hover {
    /* Effet lors du survol de la ComboBox */
    background-color: #2980b9;  /* Couleur de fond légèrement plus foncée */
    border: 2px solid #1f618d;  /* Bordure plus foncée */
}

QComboBox:pressed {
    /* Effet lors du clic sur la ComboBox */
    background-color: #1f618d;  /* Couleur de fond plus foncée */
    border: 2px solid #154360;  /* Bordure encore plus foncée */
}

QComboBox::drop-down {
    /* Style pour le bouton déroulant */
    background-color: #3498db;  /* Couleur de fond du bouton */
    border-left: 2px solid #2980b9;  /* Séparation du bouton et du champ */
    width: 30px;  /* Largeur du bouton déroulant */
}

QComboBox::down-arrow {
    /* Style pour la flèche de la ComboBox */
    image: url(:/ImgSrc/down-arrow.png);  /* Utilise une image personnalisée pour la flèche */
    width: 15px;
    height: 15px;
}

QComboBox QAbstractItemView {
    /* Style pour la liste déroulante */
    background-color: #ecf0f1;  /* Couleur de fond gris clair */
    color: #2c3e50;  /* Couleur du texte gris foncé */
    selection-background-color: #2980b9;  /* Couleur de fond de l'élément sélectionné */
    selection-color: #ffffff;  /* Couleur du texte de l'élément sélectionné */
    border: 2px solid #2980b9;  /* Bordure de la liste déroulante */
    border-radius: 5px;  /* Bords arrondis */
}

QComboBox QAbstractItemView::item {
    /* Style pour les éléments de la liste déroulante */
    padding: 5px 10px;  /* Marges intérieures des éléments */
    min-height: 25px;  /* Hauteur minimale des éléments */
}
